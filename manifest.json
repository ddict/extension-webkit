{
    "name": "Extension Testing",
    "version": "0.0.1",
    "description": "Extension Testing",
    "manifest_version": 2,
    "permissions": [
        "notifications",
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "/build/build.js",
            "/test/background.js"
        ]
    },
    "content_scripts": [{
        "matches": [
            "*://*/*",
            "file:///*/*"
        ],
        "js": [
            "/build/build.js",
            "/test/content.js"
        ]
    }],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self' https://soundcloud.hs.llnwd.net/"
}